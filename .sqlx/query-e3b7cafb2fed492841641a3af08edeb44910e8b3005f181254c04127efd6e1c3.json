{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            id,\n            name,\n            description,\n            project_id,\n            source_id,\n            created_at,\n            updated_at\n        FROM role r\n        WHERE ($9 or project_id = $1)\n            AND ($2 OR id = any($3))\n            AND ($4 OR source_id = any($5))\n            --- PAGINATION\n            AND ((r.created_at > $6 OR $6 IS NULL) OR (r.created_at = $6 AND r.id > $7))\n        ORDER BY r.created_at, r.id ASC\n        LIMIT $8\n        ",
